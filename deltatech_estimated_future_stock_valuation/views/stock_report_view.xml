<odoo>
    <record id="view_stock_quant_tree_editable_inherit" model="ir.ui.view">
        <field name="name">stock.quant.tree.editable.inherit</field>
        <field name="model">stock.quant</field>
        <field name="inherit_id" ref="stock.view_stock_quant_tree_editable" />
        <field name="arch" type="xml">
            <xpath expr="//field[@name='inventory_quantity_auto_apply']" position="after" option="show">
                <field name="forcasted_quantity" />
            </xpath>
            <xpath expr="//field[@name='value']" position="after" option="show">
                <field name="estimated_future_stock_valuation" />
            </xpath>
        </field>
    </record>
</odoo>
